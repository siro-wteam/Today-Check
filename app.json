{
  "expo": {
    "name": "TodayCheck",
    "slug": "TodayCheck",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "todaycheck",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.siro.TodayCheck"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#3B82F6",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.TodayCheck"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-screen.png",
          "resizeMode": "cover",
          "backgroundColor": "#1E3A8A",
          "enableFullScreenImage_legacy": true,
          "dark": {
            "backgroundColor": "#1E3A8A"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "TodayCheck uses your photo library to set group images and profile pictures.",
          "cameraPermission": "TodayCheck uses the camera to take photos for group images and profile pictures."
        }
      ],
      "./plugins/withNoPushEntitlement.js"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true,
      "tsConfigPaths": true
    }
  }
}